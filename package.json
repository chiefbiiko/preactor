{
  "name": "preactor",
  "version": "0.7.0",
  "description": "preactors",
  "unpkg": "./browser.js",
  "main": "./index.js",
  "scripts": {
    "bundle": "browserify ./index.js -s preactor -o ./browser.js",
    "test": "npm run node-test && npm run browser-test",
    "node-test": "tape ./test.js",
    "browser-test": "browserify ./test.browser.js | tape-run",
    "node-demo": "node ./node_usage.js",
    "browser-demo": "echo hit \"localhost:3644\" with a browser! && node -e \"var F=require('fs').readFileSync('./browser_usage.html');require('http').createServer((i,o)=>o.end(F)).listen(3644)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefbiiko/preactor.git"
  },
  "keywords": [
    "preactor",
    "preactors",
    "events",
    "event handling",
    "reactive programming",
    "event-driven programming",
    "EventEmitter"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiefbiiko/preactor/issues"
  },
  "homepage": "https://github.com/chiefbiiko/preactor#readme",
  "devDependencies": {
    "browserify": "16.2.2",
    "debug": "3.1.0",
    "tape": "4.9.1",
    "tape-run": "4.0.0"
  }
}
